<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_01"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="72dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        app:layout_constraintTop_toBottomOf="@+id/tbox_01" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_03"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_01" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_05"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="12dp"
        android:hint=""
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.281"
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_03" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_07
"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_05" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_06"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/tbox_val_05"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_04" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_08
"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/tbox_val_07
"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_06" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_04"
        android:layout_width="115dp"
        android:layout_height="43dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/tbox_val_03"
        app:layout_constraintTop_toBottomOf="@+id/tbox_val_02" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_val_02"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="72dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/tbox_val_01"
        app:layout_constraintTop_toBottomOf="@+id/tbox_01" />

    <Button
        android:id="@+id/btn_save"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="80dp"
        android:text="Save"
        app:layout_constraintTop_toBottomOf="@+id/btn_input"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/btn_Load"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Load"
        app:layout_constraintTop_toBottomOf="@+id/btn_save"
        tools:layout_editor_absoluteX="16dp" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_01"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="52dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        app:layout_constraintTop_toBottomOf="@+id/tbox_02" />

    <android.support.design.widget.TextInputEditText
        android:id="@+id/tbox_02"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:hint=""
        app:layout_constraintStart_toEndOf="@+id/btn_save"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/btn_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

</android.support.constraint.ConstraintLayout>